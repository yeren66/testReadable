{
  "version": 2,
  "name": "test-case-evaluation-system",
  "builds": [
    {
      "src": "index_with_data.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index_with_data.html"
    },
    {
      "src": "/app.js",
      "dest": "/app_with_data.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/save-evaluation.js": {
      "runtime": "nodejs18.x"
    }
  }
}
